{
  "tag": "VLESS-HTTPUPGRADE-TLS",
  "listen": "0.0.0.0",
  "port": 2087,
  "protocol": "vless",
  "settings": {
    "clients": [],
    "decryption": "none",
    "fallbacks": []
  },
  "streamSettings": {
    "network": "httpupgrade",
    "security": "tls",
    "tlsSettings": {
      "serverName": "",
      "alpn": ["http/1.1"],
      "minVersion": "1.2",
      "certificates": [
        {
          "certificateFile": "/var/lib/marzban/certs/fullchain.pem",
          "keyFile": "/var/lib/marzban/certs/privkey.pem"
        }
      ]
    },
    "httpupgradeSettings": {
      "path": "/vless-httpupgrade",
      "host": "",
      "maxEarlyData": 2048,
      "earlyDataHeaderName": "Sec-WebSocket-Protocol"
    }
  },
  "sniffing": {
    "enabled": true,
    "destOverride": ["http", "tls", "quic"],
    "routeOnly": false
  }
}
