{
  "tag": "VMESS-HTTPUPGRADE-TLS",
  "listen": "0.0.0.0",
  "port": 2096,
  "protocol": "vmess",
  "settings": {
    "clients": []
  },
  "streamSettings": {
    "network": "httpupgrade",
    "security": "tls",
    "tlsSettings": {
      "serverName": "",
      "alpn": ["http/1.1"],
      "minVersion": "1.2",
      "certificates": [
        {
          "certificateFile": "/var/lib/marzban/certs/fullchain.pem",
          "keyFile": "/var/lib/marzban/certs/privkey.pem"
        }
      ]
    },
    "httpupgradeSettings": {
      "path": "/vmess-httpupgrade",
      "host": "",
      "maxEarlyData": 1024,
      "earlyDataHeaderName": "Sec-WebSocket-Protocol"
    }
  },
  "sniffing": {
    "enabled": true,
    "destOverride": ["http", "tls"],
    "routeOnly": false
  }
}
